package test;

import java.util.*;

public class VectorDemo {

	public static void main(String[] args) {
		
		Vector <String> vector= new Vector <String>();
		vector.add("mango");
		vector.add("Apple");
		vector.add("guava");
		vector.add("grapes");
		System.out.println("vector list - "+vector);
		vector.add(2,"Banana");
		System.out.println("vector list after adding banana - "+vector);
		vector.remove(2);
		System.out.println("vector list after removing banana - "+vector);
		for(int i=0;i<vector.size();i++)
		{
			System.out.println(vector.get(i));
		}
		//System.out.println(vector);
		//System.out.println(vector);

	}

}
